{
    "name": "webpack-sample",
    "displayName": "webpack-sample",
    "description": "This is an extension that uses [https://webpack.js.org]() to bundle and minify its sources. Using webpack will help to reduce the install- and startup-time of large extensions because instead of hundreds of files, a single file is produced.",
    "version": "0.0.1",
    "publisher": "vscode-samples",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Microsoft/vscode-extension-samples.git"
    },
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.helloWebpack",
                "title": "Hello Webpack"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "test-compile": "tsc -p ./",
        "lint": "eslint"
    },
    "devDependencies": {
        "@babel/core": "^7.27.4",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-private-methods": "^7.18.6",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@babel/plugin-transform-class-properties": "^7.27.1",
        "@babel/plugin-transform-private-methods": "^7.27.1",
        "@babel/preset-env": "^7.27.2",
        "@eslint/js": "^9.13.0",
        "@stylistic/eslint-plugin": "^2.9.0",
        "@types/node": "^20",
        "@types/vscode": "^1.73.0",
        "babel-loader": "^10.0.0",
        "css-loader": "^7.1.2",
        "eslint": "^9.13.0",
        "sass": "^1.89.2",
        "sass-loader": "^16.0.5",
        "style-loader": "^4.0.0",
        "ts-loader": "^7.0.5",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.0",
        "webpack": "^5.99.9",
        "webpack-cli": "^5.1.4"
    },
    "keywords": [],
    "author": "",
    "bugs": {
        "url": "https://github.com/Microsoft/vscode-extension-samples/issues"
    },
    "homepage": "https://github.com/Microsoft/vscode-extension-samples#readme",
    "type": "module"
}
